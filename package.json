{
  "name": "app",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:android": "ionic capacitor open android",
    "build": "vue-tsc && vite build",
    "build:ios": "vue-tsc && vite build && cap sync ios",
    "build:android": "vue-tsc && vite build && cap sync android",
    "build:debug:android": "cd android && ./gradlew assembleDebug && cp app/build/outputs/apk/debug/app-debug.apk ../DEBUG.APK",
    "build:simu": "build:debug:android && adb install -r DEBUG.APK",
    "preview": "vite preview",
    "test:e2e": "cypress run",
    "test:unit": "vitest",
    "lint": "eslint .",
    "pretty": "prettier --write ."
  },
  "dependencies": {
    "@capacitor/android": "5.7.1",
